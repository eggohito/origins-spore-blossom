            ]
        }
    },
    "drown": {
        "type": "origins:damage_over_time",
        "interval": 20,
        "damage": 2,
        "damage_easy": 1,
        "damage_source": {
            "name": "drown",
            "bypasses_armor": true,
            "unblockable": true
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_bar",
            "comparison": "==",
            "compare_to": 100
        }
    },
    "dryout": {
        "type": "origins:damage_over_time",
        "interval": 20,
        "damage": 2,
        "damage_easy": 1,
        "damage_source": {
            "name": "natural_dryout",
            "bypasses_armor": true,
            "unblockable": true
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_bar",
            "comparison": "==",
            "compare_to": 0
        }
    },
    "respawn": {
        "type": "origins:action_on_callback",
        "entity_action_respawned": {
            "type": "origins:execute_command",
            "command": "resource set @s origins-spore-blossom:moisture_bar 50"
        }
    },
    "name": "Moisture",
    "description": "Due to your plant nature, being too dry or wet can kill you. \n\n* Water sources, rain and drinking potions can hydrate you whilst blocks that can be lit (or fire/lava) can dry you."
}